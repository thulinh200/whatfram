task.spawn(function()
for i = 1, PlayerInServer do
	for ii = 1,#alts do
        if getPlayers[i].Name == alts[ii] and alts[ii] ~= plrs.LocalPlayer.Name then
        	while true do
				serverhop()
			task.wait(5)
	    	end
        end
    end
end
end)

plrs.PlayerAdded:Connect(function(player)

	task.wait()
	for i = 1,#alts do
		if player.Name == alts[i] and alts[i] ~= plrs.LocalPlayer.Name then
			task.wait(1)
	    	serverhop()
			
			
        end
    end
end)





