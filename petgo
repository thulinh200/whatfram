-- Function to get a random username from the list
local function getRandomUsername()
    return randomUsernames[math.random(1, #randomUsernames)]
end

-- Ensure petsGoConfig is initialized before accessing
getgenv().petsGoConfig = getgenv().petsGoConfig or {} -- Make sure petsGoConfig exists
getgenv().petsGoConfig.USERNAME_TO_MAIL = getRandomUsername() -- Set random username

script_key="kuQoDnFgeRipnysIrKrGhcoLjpYFSUEn";

